{
  "name": "@vflowx/unocss-primevue",
  "type": "module",
  "version": "0.0.2-beta.1",
  "exports": {
    ".": {
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "bun --watch run src/index.ts",
    "b": "unbuild",
    "p": "pnpm pack --pack-destination ../web-template/lib/",
