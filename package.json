    "pub": "bumpp --no-commit --no-tag --no-push && npm publish"
  },
  "peerDependencies": {
    "unocss": "^0.62.2"
  },
  "publishConfig": {
    "registry": "https://dieuhanh.nbca.gov.vn/gitea/api/packages/vflowx/npm/",
    "linkDirectory": true
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.27.3",
    "@types/bun": "latest",
    "bumpp": "^9.5.2",
    "eslint": "^9.9.1",
    "taze": "^0.16.7",
    "typescript": "^5.5.4",
    "unbuild": "^2.0.0"
  }
}
